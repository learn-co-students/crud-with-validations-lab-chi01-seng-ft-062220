<h1>All Songs</h1>
<table>
    <thead>
    <tr>
        <th>Title</th>
        <th>Artist</th>
    </tr>
        <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= link_to song.title, song_path(song) %></td>
                <td><%= song.artist_name %></td>
        <% end %>
            </tr>
        </tbody>
</table><br>

<%= button_to "Add A Song", new_song_path, method: :get %>

